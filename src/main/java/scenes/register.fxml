<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<?import java.lang.String?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="container" stylesheets="@../styles/register.css"
            xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controllers.Register">
   <AnchorPane layoutX="163.0" layoutY="85.0" prefHeight="230.0" prefWidth="274.0" styleClass="register-inputs">
      <Button layoutX="221.0" mnemonicParsing="false" onMouseClicked="#returnToPreviousScene">
         <styleClass>
            <String fx:value="close-btn"/>
            <String fx:value="btn"/>
         </styleClass>
      </Button>
      <TextField fx:id="nameInput" layoutX="21.0" layoutY="23.0" prefHeight="28.0" prefWidth="232.0"
                 promptText="Name" styleClass="input"/>
      <TextField fx:id="passwordInput" layoutX="21.0" layoutY="99.0" prefHeight="28.0" prefWidth="232.0"
                 promptText="Password" styleClass="input"/>
      <TextField fx:id="emailInput" layoutX="21.0" layoutY="62.0" prefHeight="28.0" prefWidth="232.0"
                 promptText="Email" styleClass="input"/>
      <TextField fx:id="confirmPasswordInput" layoutX="21.0" layoutY="136.0" prefHeight="28.0" prefWidth="232.0"
                 promptText="Repeat password" styleClass="input"/>
      <Button layoutX="106.0" layoutY="188.0" mnemonicParsing="false"
              onMouseClicked="#validateRegistration" styleClass="register-btn" text="Register"/>
   </AnchorPane>
</AnchorPane>
